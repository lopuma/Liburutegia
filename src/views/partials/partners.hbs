<!--Modal para CRUD-->
<div id="modalPARTNER" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="modalPARTNERLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalPARTNERLabel"></h5>
      </div>
      <div class="New">
        <form class="New-form" id="formu">
          {{> formNew}}
          <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal" tabindex="10">Cancel</button>
            <button type="submit" id="btnGuardar" class="btn btn-dark" tabindex="11">Save</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

{{!-- LOADING --}}
<div id="spinner">
  <div class="lds-dual-ring">&nbsp</div>
</div>

