{{> modalDeliver}}
<div class="Dashboard" id="stateBookings" style="margin-top: 7em;">
    <a class="Dashboard-a" href="#" hidden><i class="Dashboard-i fa-solid fa-user-plus"></i>New Booking</a>
    <div class="Dashboard-row relative">
        <div id="spinner">
            {{> loading}}
        </div>
        {{> comButtonClose
        id="btnClearBookings"
        className="absolute top-5 right-0 z-50 invisible"
        }}
        <table id="tableBookings" class="Dashboard-table table table-bordered  display nowrap" cellspacing="0"
            width="100%">
            <thead class="Dashboard-head">
                <tr>
                    <th>BOOKING ID</th>
                    <th>BOOK ID</th>
                    <th>TITLE</th>
                    <th>ISBN</th>
                    <th>PARTNER DNI</th>
                    <th>FULL NAME</th>
                    <th>RESERVATION DATE</th>
                    <th>DELIVERED DATE</th>
                    <th>STATUS</th>
                    <th>OBSERVATION</th>
                    <th>ACTIONS</th>
                </tr>
            </thead>
            <tbody id="tbodyBookings">
            </tbody>
            <tfoot class="Dashboard-foot">
                <tr>
                    <th>BOOKING ID</th>
                    <th>BOOK ID</th>
                    <th>TITLE</th>
                    <th>ISBN</th>
                    <th>PARTNER DNI</th>
                    <th>FULL NAME</th>
                    <th>RESERVATION DATE</th>
                    <th>DELIVERED DATE</th>
                    <th>STATUS</th>
                    <th>OBSERVATION</th>
                    <th>ACTIONS</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>
<script defer src="/scripts/Globals/funcGlobals.js" type="text/javascript"></script>
<script defer src="/scripts/bookings/bookings.js"></script>