<div class="Workspace-container" id="stateBooks">
  <fieldset class="Workspace-fieldset">
    <legend class="Workspace-legend isColorBook" id="lengedWorkspace">
      <span class="Workspace-span">Book Dashboard</span>
      {{#if loggedIn}}
      <div class="Workspace-Username Username">
        <span class="Username-greeting">Hi, Welcome </span>
        <span class="Username-name" id="tipName">{{userName}}</span>
      </div>
      {{/if}}
    </legend>
    {{> books }}
  </fieldset>
</div>

<link rel="stylesheet" href="/style/workspace.css">